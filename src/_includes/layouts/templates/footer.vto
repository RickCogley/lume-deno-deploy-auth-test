    <div class="links">
      {{ set lastKey = footer.links.length - 1 }}
      {{ for key, item of footer.links }}
        <a class="menu-link" href="{{ item.url }}" aria-label="Nav link in footer">{{ item.text }}</a> {{ if key == lastKey }}{{else}}|{{ /if }}
      {{ /for }}
    </div>
